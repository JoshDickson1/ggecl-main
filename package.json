{
  "name": "ggecl-app",
  "packageManager": "pnpm@10.7.1",
  "engines": {
    "node": "23.x"
  },
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "backend",
    "frontend"
  ],
  "scripts": {
    "dev": "pnpm run -r --parallel --aggregate-output dev",
    "build": "pnpm run -r build"
  },
  "dependencies": {
    "@trpc/server": "^11.0.2",
    "superjson": "^2.2.2"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "react-day-picker>react": "19",
        "react-day-picker>date-fns": "4"
      }
    }
  }
}
